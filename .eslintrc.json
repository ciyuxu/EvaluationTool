{
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error"
  },

  "extends": ["prettier"],
  "env": { "es6": true },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  }
}
